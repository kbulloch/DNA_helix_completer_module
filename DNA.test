<?php

class DNATests extends DrupalUnitTestCase {

    public static function getInfo() {
        return array(
            'name' => 'DNA Test',
            'description' => 'Test the DNA completer function',
            'group' => 'Aardvark',
        );
    }

    function testDNA() {
        $result = DNA('atCG');
        $second = 'TAGC';
        $this->assertEqual($result, $second);
    }
}
